#ifndef _LIBCPP_STDLIB_SUPPORT
#define _LIBCPP_STDLIB_SUPPORT

#include <__config>

#ifndef _LIBCPP_HAS_NO_PRAGMA_SYSTEM_HEADER
#pragma GCC system_header
#endif

#ifndef __libcpp_has_include
  #ifndef __has_include
    #define __libcpp_has_include(x) 0
  #else
    #define __libcpp_has_include(x) __has_include(x)
  #endif
#endif

#if defined(_LIBCPP_HAS_STDLIB_API_EXTERNAL) && \
    __libcpp_has_include(<__external_stdlib>)
#include <__external_stdlib>
#else

#include_next <stdlib.h>

_LIBCPP_BEGIN_NAMESPACE_STD

// inline _LIBCPP_INLINE_VISIBILITY

_LIBCPP_END_NAMESPACE_STD

#endif

#endif // _LIBCPP_STRING_SUPPORT
