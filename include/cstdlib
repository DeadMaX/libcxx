#ifndef _LIBCPP_CSTDLIB
#define _LIBCPP_CSTDLIB

/*
    cstdlib synopsis

Macros:

    EXIT_FAILURE
    EXIT_SUCCESS
    MB_CUR_MAX
    NULL
    RAND_MAX

namespace std
{

Types:

    size_t
    div_t
    ldiv_t
    lldiv_t                                                               // C99

double    atof (const char* nptr);
int       atoi (const char* nptr);
long      atol (const char* nptr);
long long atoll(const char* nptr);                                        // C99
double             strtod  (const char* restrict nptr, char** restrict endptr);
float              strtof  (const char* restrict nptr, char** restrict endptr); // C99
long double        strtold (const char* restrict nptr, char** restrict endptr); // C99
long               strtol  (const char* restrict nptr, char** restrict endptr, int base);
long long          strtoll (const char* restrict nptr, char** restrict endptr, int base); // C99
unsigned long      strtoul (const char* restrict nptr, char** restrict endptr, int base);
unsigned long long strtoull(const char* restrict nptr, char** restrict endptr, int base); // C99
int rand(void);
void srand(unsigned int seed);
void* calloc(size_t nmemb, size_t size);
void free(void* ptr);
void* malloc(size_t size);
void* realloc(void* ptr, size_t size);
void abort(void);
int atexit(void (*func)(void));
void exit(int status);
void _Exit(int status);
char* getenv(const char* name);
int system(const char* string);
void* bsearch(const void* key, const void* base, size_t nmemb, size_t size,
              int (*compar)(const void *, const void *));
void qsort(void* base, size_t nmemb, size_t size,
           int (*compar)(const void *, const void *));
int         abs(      int j);
long        abs(     long j);
long long   abs(long long j);                                             // C++0X
long       labs(     long j);
long long llabs(long long j);                                             // C99
div_t     div(      int numer,       int denom);
ldiv_t    div(     long numer,      long denom);
lldiv_t   div(long long numer, long long denom);                          // C++0X
ldiv_t   ldiv(     long numer,      long denom);
lldiv_t lldiv(long long numer, long long denom);                          // C99
int mblen(const char* s, size_t n);
int mbtowc(wchar_t* restrict pwc, const char* restrict s, size_t n);
int wctomb(char* s, wchar_t wchar);
size_t mbstowcs(wchar_t* restrict pwcs, const char* restrict s, size_t n);
size_t wcstombs(char* restrict s, const wchar_t* restrict pwcs, size_t n);
int at_quick_exit(void (*func)(void))                                     // C++11
void quick_exit(int status);                                              // C++11
void *aligned_alloc(size_t alignment, size_t size);                       // C11

}  // std

*/

#include <__config>
#include <stdlib.h>

#if !defined(_LIBCPP_HAS_NO_PRAGMA_SYSTEM_HEADER)
#pragma GCC system_header
#endif

#ifdef __GNUC__
#define _LIBCPP_UNREACHABLE() __builtin_unreachable()
#else
#define _LIBCPP_UNREACHABLE() _VSTD::abort()
#endif

_LIBCPP_BEGIN_NAMESPACE_STD

using _LIBCPP_CNAMESPACE::size_t;
using _LIBCPP_CNAMESPACE::div_t;
using _LIBCPP_CNAMESPACE::ldiv_t;
#ifndef _LIBCPP_HAS_NO_LONG_LONG
using _LIBCPP_CNAMESPACE::lldiv_t;
#endif // _LIBCPP_HAS_NO_LONG_LONG
using _LIBCPP_CNAMESPACE::atof;
using _LIBCPP_CNAMESPACE::atoi;
using _LIBCPP_CNAMESPACE::atol;
#ifndef _LIBCPP_HAS_NO_LONG_LONG
using _LIBCPP_CNAMESPACE::atoll;
#endif // _LIBCPP_HAS_NO_LONG_LONG
using _LIBCPP_CNAMESPACE::strtod;
using _LIBCPP_CNAMESPACE::strtof;
using _LIBCPP_CNAMESPACE::strtold;
using _LIBCPP_CNAMESPACE::strtol;
#ifndef _LIBCPP_HAS_NO_LONG_LONG
using _LIBCPP_CNAMESPACE::strtoll;
#endif // _LIBCPP_HAS_NO_LONG_LONG
using _LIBCPP_CNAMESPACE::strtoul;
#ifndef _LIBCPP_HAS_NO_LONG_LONG
using _LIBCPP_CNAMESPACE::strtoull;
#endif // _LIBCPP_HAS_NO_LONG_LONG
using _LIBCPP_CNAMESPACE::rand;
using _LIBCPP_CNAMESPACE::srand;
using _LIBCPP_CNAMESPACE::calloc;
using _LIBCPP_CNAMESPACE::free;
using _LIBCPP_CNAMESPACE::malloc;
using _LIBCPP_CNAMESPACE::realloc;
using _LIBCPP_CNAMESPACE::abort;
using _LIBCPP_CNAMESPACE::atexit;
using _LIBCPP_CNAMESPACE::exit;
using _LIBCPP_CNAMESPACE::_Exit;
#ifndef _LIBCPP_WINDOWS_STORE_APP
using _LIBCPP_CNAMESPACE::getenv;
using _LIBCPP_CNAMESPACE::system;
#endif
using _LIBCPP_CNAMESPACE::bsearch;
using _LIBCPP_CNAMESPACE::qsort;
using _LIBCPP_CNAMESPACE::abs;
using _LIBCPP_CNAMESPACE::labs;
#ifndef _LIBCPP_HAS_NO_LONG_LONG
using _LIBCPP_CNAMESPACE::llabs;
#endif // _LIBCPP_HAS_NO_LONG_LONG
using _LIBCPP_CNAMESPACE::div;
using _LIBCPP_CNAMESPACE::ldiv;
#ifndef _LIBCPP_HAS_NO_LONG_LONG
using _LIBCPP_CNAMESPACE::lldiv;
#endif // _LIBCPP_HAS_NO_LONG_LONG
using _LIBCPP_CNAMESPACE::mblen;
using _LIBCPP_CNAMESPACE::mbtowc;
using _LIBCPP_CNAMESPACE::wctomb;
using _LIBCPP_CNAMESPACE::mbstowcs;
using _LIBCPP_CNAMESPACE::wcstombs;
#ifdef _LIBCPP_HAS_QUICK_EXIT
using _LIBCPP_CNAMESPACE::at_quick_exit;
using _LIBCPP_CNAMESPACE::quick_exit;
#endif
#ifdef _LIBCPP_HAS_C11_FEATURES
using _LIBCPP_CNAMESPACE::aligned_alloc;
#endif

_LIBCPP_END_NAMESPACE_STD

#endif  // _LIBCPP_CSTDLIB
